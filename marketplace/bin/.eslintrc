{
  "extends": "airbnb",
  "env": {
    "node": true
  },
  "rules": {
    "strict": 0,
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-console": 0,
    "no-param-reassign": 0,
    "max-len": [0, { code: 120, tabWidth: 4, ignoreUrls: true }],
    "new-cap": 0,
    "consistent-return": 0,
    "no-unused-vars": 0,
  }
}
