{
  "name": "marketplace",
  "version": "1.1.0",
  "description": "Digital Dimension Marketplace",
  "main": "bin/index.js",
  "scripts": {
    "start": "NODE_ENV=production nodemon .",
    "start:dev": "NODE_ENV=development nodemon .",
    "start:dev:osx": "nodemon . --config nodemon.osx.json",
    "webpack:osx": "webpack-dev-server --no-info --config webpack.config.osx.js",
    "webpack": "webpack-dev-server --no-info --config webpack.config.js",
    "webpack:docker": "webpack -w --devtool source-map --config webpack.config.docker.js",
    "build-webpack-dev": "webpack --config webpack.config.js --progress --profile --color",
    "build-webpack": "NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --color",
    "build-gulp": "NODE_ENV=production NODE_PATH=NODE_PATH:./dist/shared:./dist/server gulp build",
    "build-gulp-production": "NODE_ENV=production NODE_PATH=NODE_PATH:./dist/shared:./dist/server gulp build-production",
    "watch-gulp": "NODE_ENV=production NODE_PATH=NODE_PATH:./src/shared:./src/server gulp watch",
    "test": "NODE_PATH=NODE_PATH:./dist/shared:./dist/server mocha --compilers js:babel-core/register --require ./tests/test_helper.js  --recursive tests",
    "test:permissions": "NODE_PATH=NODE_PATH:./dist/shared:./dist/server mocha --compilers js:babel-core/register ./tests/helper-functions/permissions.js",
    "build": "npm run babelify && npm run build-gulp-production && npm run build-webpack",
    "babelify": "babel src/ --out-dir dist/ --copy-files",
    "server-start": "API_V02_ENDPOINT=\"http://192.168.0.113:8080/\" NODE_ENV=production PORT=8000 NODE_PATH=NODE_PATH:./dist/shared:./dist/server node bin/index.js",
    "server-stop": "pm2 stop marketplace",
    "server-restart": "pm2 restart marketplace",
    "tenant": "NODE_PATH=NODE_PATH:./src/shared:./src/server node bin/create-tenant",
    "eslint": "eslint src/*.js*",
    "migrate": "NODE_PATH=NODE_PATH:./src/shared:./src/server node --harmony bin/migrate.js",
    "migrate:create": "npm run migrate create",
    "migrate:rollback": "npm run migrate rollback",
    "migrate:pending": "npm run migrate pending",
    "nightwatch": "nightwatch --test nightwatch-tests/index.js",
    "create-module": "node bin/create-module"
  },
